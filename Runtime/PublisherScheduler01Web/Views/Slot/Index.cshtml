@model IEnumerable<PublisherScheduler01Web.ViewModels.SlotViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SlotDetail.Begin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SlotDetail.End)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LocationName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SlotDetail.IsActive)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SlotDetail.Begin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SlotDetail.End)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LocationName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SlotDetail.IsActive)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SlotDetail.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.SlotDetail.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SlotDetail.Id })
        </td>
    </tr>
}

</table>
